{
    "name":"asconsulting/pinball_database",
    "description":"Pinball Database for Contao",
    "homepage": "https://andrewstevens.consulting",
    "keywords":["contao", "pinball", "archive", "ipdb"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Andrew Stevens",
            "homepage":"https://andrewstevens.consulting"
        }
    ],
    "require":{
        "php": ">=7.2",
        "contao/core-bundle": ">=4.4"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-0": {
            "WalrusPinball\\": [
                "system/modules/pinball_database/library"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/pinball_database": "system/modules/pinball_database"
            }
        }
    },
    "minimum-stability": "dev"
}